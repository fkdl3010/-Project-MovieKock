<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.koreait.movie.dao.Main_dao">
	
	<select id="my_write" resultType="com.koreait.movie.dto.CommentDto">
		SELECT user_no, movie_no, comments_title, comment_date
		  FROM comments
		  where user_no = #{user_no}
	</select>
	
	
	
	<select id="view" parameterType="int" resultType="xx.xx.xx.xx.Dto">
   		Select * 
   			from comment 
   			where no=#{no}
	</select>
	
	
	
	
</mapper>