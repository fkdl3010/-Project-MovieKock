<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.koreait.movie.dao.MoveDao">
	
	
	<select id="movieNo" parameterType="int" resultType="int">
		select movie_no
		from movie
		 WHERE movie_no = #{choiceMovieNo}
	</select>
	
	<select id="movielist" resultType="com.koreait.movie.dto.MovieDto">
		select *
		from movie
	</select>
	
	
	<select id="clickMovieNo" resultType="int">
		select *
		from movie
		where movie_no = #{clickMovieNo}
	</select>

</mapper>